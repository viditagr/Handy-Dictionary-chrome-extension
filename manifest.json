{
    "name": "Handy Dictionary",
    "description": "This extension makes it easier to check the meaning of various words without having to go to a different website to check it.",
    "version": "1.0",
    "manifest_version": 3,
      "permissions": ["storage", "activeTab", "scripting"],
      "action": {
        "default_popup": "popup.html"
      },
      "icons": {
        "16": "/images/dict_16x16.png",
        "32": "/images/dict_32x32.png",
        "48": "/images/dict_48x48.png",
        "128": "/images/dict_128x128.png"
      }
  }
